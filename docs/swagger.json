                "name": {
                    "type": "string"
                },
                "ownerID": {
                    "type": "string"
                }
            }
        },
        "model.GroupMember": {
            "type": "object",
            "required": [
                "groupID",
                "userID"
            ],
            "properties": {
                "confirmedTime": {
                    "type": "string"
                },
                "group": {
                    "$ref": "#/definitions/model.Group"
                },
                "groupID": {
                    "type": "integer"
                },
                "user": {
                    "$ref": "#/definitions/model.User"
                },
                "userID": {
                    "type": "integer"
                }
            }
        },
        "model.User": {
            "type": "object",
