                },
                "friendID": {
                    "type": "integer"
                },
                "requestAt": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                },
                "user": {
                    "$ref": "#/definitions/model.User"
                },
                "userID": {
                    "type": "integer"
                }
            }
        },
        "model.Group": {
            "type": "object",
